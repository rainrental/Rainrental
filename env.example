# RainRental RFID App Configuration
# Copy this file to .env and modify the values below

# App Configuration
PACKAGE_NAME=com.yourcompany.yourapp
APP_NAME=Your RFID App
COMPANY_ID=your_company_id

# Output Configuration (for generate_app.sh)
OUTPUT_DIR=../generated-app
PROJECT_NAME=MyRfidApp

# Build Configuration
BUILD_DEBUG=true
BUILD_RELEASE=true

# Device Deployment
DEPLOY_TO_DEVICE=false

# Optional: Custom API Configuration
# API_BASE_URL=https://your-api.com/api/v1/
# API_KEY=your_api_key_here

# Optional: MQTT Configuration
# MQTT_BROKER=your-mqtt-broker.com
# MQTT_PORT=1883
# MQTT_USERNAME=your_username
# MQTT_PASSWORD=your_password

# Optional: Firebase Configuration
# FIREBASE_PROJECT_ID=your-firebase-project
# FIREBASE_APP_ID=your-firebase-app-id 